<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:name="shape_jugabilidad">
        <shape>
            <solid android:color="@android:color/holo_green_light" />
            <stroke android:width="4dp" android:color="@color/black" />
            <!--corners android:radius="355dp"/-->
            <corners android:bottomLeftRadius="100dp" android:bottomRightRadius="100dp" android:topLeftRadius="100dp" android:topRightRadius="100dp" />
        </shape>
    </item>

    <item android:id="@+id/shape_jugabilidad2_general">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <solid android:color="@color/white" />
            <stroke android:width="4dp" android:color="@android:color/holo_green_light" />
            <!--corners android:radius="355dp"/-->
            <corners android:bottomLeftRadius="100dp" android:bottomRightRadius="100dp" android:topLeftRadius="100dp" android:topRightRadius="100dp" />
        </shape>
    </item>

    <item></item>

    <item></item>

    <item></item>

    <item></item>
</selector>